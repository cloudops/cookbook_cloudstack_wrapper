filebeat.prospectors:

- input_type: log

  paths:
    - /var/log/cloudstack/management/management-server.log
  multiline.pattern: '^(-?(?:[1-9][0-9]*)?[0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])[T\s](2[0-3]|[01][0-9]):([0-5][0-9]):([0-5][0-9])(\.[0-9]+)?(Z|[+-](?:2[0-3]|[01][0-9]):[0-5][0-9])?'

  multiline.negate: true

  multiline.match: after
output.logstash:
  hosts: ["<%= @facility %>"]

